<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#F9FCFF"/>
        </shape>
    </item>
    <item
        android:top="1dp" android:bottom="1dp"
        android:left="-1dp" android:right="-1dp">
        <shape android:shape="rectangle">
            <stroke android:color="#E2E8F0" android:width="1dp" android:dashWidth="@dimen/dimen_8" android:dashGap="@dimen/dimen_8"/>
            <solid android:color="#00000000"/>
        </shape>
    </item>
</layer-list>